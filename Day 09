package BasicJavaProgram;

import java.util.Scanner;

public class BinaryToDecimal {
	public static void main(String[] args) {
		
		Scanner s= new Scanner(System.in);
		int n=s.nextInt();

		int x=1;
		int ans=0;
    int y=0;

		while(n>0){
          
		  y=n%10;
			ans=ans+y*x;

			x=x*2;

			n=n/10;


		}

		System.out.println(ans);

	}
}


package BasicJavaProgram;

import java.util.Scanner;

public class SquareRoot {
	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
        int n=s.nextInt();

		int i=1;

		while(i*i<=n){

           i++;

		}

          i=i-1;
		System.out.print(i);



	}

}

