import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		
		Scanner s= new Scanner(System.in);
		int n=s.nextInt();

		int i=1;
		
		while(i<=n){

		
            int StartChar='A'+i-1;
				int j=1;
			while(j<=i){
				
          System.out.print((char)(StartChar));
		  j++;
		  StartChar++;

			}
			System.out.println();

			i++;
		}

		
	}

}
