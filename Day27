import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		//Your code goes here
        	
		Scanner s = new Scanner (System.in);
        
        int n = s.nextInt();
        
        int i=n;
    
        while( i>=1)
            
        {
        int j=i;
             int  Start = 'A'+j-1;
            while(j<=n)
        {
             
            
            System.out.print((char)(Start));
                Start++;
                j++;
                
        }
         System.out.println();
          i--;
		
	}


	}

}

