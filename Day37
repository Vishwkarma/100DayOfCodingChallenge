public class Solution {

	public static void leaders(int[] arr) {
		int size=arr.length;
		for (int i = 0; i < size; i++)  

        { 

            int j; 

            for (j = i + 1; j < size; j++)  

            { 

                if (arr[i] < arr[j]) 

                    break; 

            } 

            if (j == size) 

                System.out.print(arr[i] + " "); 

        } 

    }
	}
